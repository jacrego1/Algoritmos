def analizar_url(url):
    # Empezamos con valores vac√≠os

    protocolo = ""
    dominio = ""
    recurso = None

# Sacamos lo primero que es el protocolo

    if "://" in url:
        partes = url.split("://")
        protocolo = partes[0]
        resto = partes[1]
    else:
        print("URL sin protocolo")
        return None, None, None

# Sacamos el dominio de la url

    if "/" in resto:
        partes2 = resto.split("/", 1)
        dominio = partes2[0]
        ruta = partes2[1]
    else:
        dominio = resto
        ruta = ""

#Sacamos el recurso en caso de que lo haya

    if ruta != "":
        trozos = ruta.split("/")
        recurso = trozos[-1]

    return protocolo, dominio, recurso


# Ejemplo:
url = "https://www.google.com/search/index.html"
p, d, r = analizar_url(url)
print("Protocolo:", p)
print("Dominio:", d)
print("Recurso:", r)
 